{
  "name": "t63",
  "version": "1.4.4",
  "description": "Simple Basic React Components",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.es.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c ./rollup.config.js -n t63",
    "test": "NODE_ENV=test babel-tape-runner \"src/**/*test.js\" | faucet",
    "dev": "rollup -c -w ./rollup.config.js -n t63",
    "start": "node server.js"
  },
  "keywords": [],
  "author": "Tom",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "enzyme": "^2.9.1",
    "faucet": "0.0.1",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1",
    "tape": "^4.7.0"
  },
  "dependencies": {
    "babel-helpers": "^6.24.1",
    "cuid": "^1.3.8",
    "ramda": "^0.24.1",
    "react-icons": "^2.2.5"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015",
          "react"
        ]
      },
      "production": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "react"
        ],
        "plugins": [
          "external-helpers"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilson63/t63.git"
  },
  "bugs": {
    "url": "https://github.com/twilson63/t63/issues"
  },
  "homepage": "https://github.com/twilson63/t63#readme"
}
